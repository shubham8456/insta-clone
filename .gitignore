Network Trash Folder
Temporary Items
.apdisk

# Ignore bundler config.
/instagram/.bundle

# Ignore the default SQLite database.
/instagram/db/*.sqlite3
/instagram/db/*.sqlite3-*

# Ignore all logfiles and tempfiles.
/instagram/log/*
/instagram/tmp/*
!/instagram/log/.keep
!/instagram/tmp/.keep

# Ignore pidfiles, but keep the directory.
/instagram/tmp/pids/*
!/instagram/tmp/pids/
!/instagram/tmp/pids/.keep

# Ignore uploaded files in development.
/instagram/storage/*
!/instagram/storage/.keep
/instagram/tmp/storage/*
!/instagram/tmp/storage/
!/instagram/tmp/storage/.keep

# Ignore local uploads
/instagram/public/uploads/*
!/instagram/public/uploads/

# Ignore master key for decrypting credentials and more.
/instagram/config/master.key

# Ignore application configuration
/instagram/config/application.yml

